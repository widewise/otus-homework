{
  "name": "city-weather",
  "version": "1.0.0",
  "description": "Task 9 homework",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/widewise/otus-homework.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      ["@babel/preset-react", {
        "runtime": "automatic"
      }]
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "transform": {
      "\\.[jt]sx?$": "esbuild-jest"
    },
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/__mock__/CSSStub.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "author": "Aleksandr Mulgachev",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.5.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.4"
  }
}
